{
  "name": "clear-aligner",
  "version": "0.0.30",
  "license": "MIT",
  "private": false,
  "homepage": ".",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/roboto": "^4.5.5",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.7.0",
    "@mui/x-data-grid": "^6.18.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@rgrove/parse-xml": "^3.0.0",
    "@uidotdev/usehooks": "^2.4.1",
    "better-sqlite3": "^9.4.3",
    "cache-manager": "^5.4.0",
    "electron-is-dev": "^2.0.0",
    "queue-promise": "^2.2.1",
    "react-hotkeys-hook": "^4.5.0",
    "react-redux": "^7.2.8",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.0",
    "redux-undo": "^1.0.1",
    "reflect-metadata": "^0.2.1",
    "sanitize-filename": "^1.6.3",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.9.2",
    "typeorm": "^0.3.20",
    "typescript": "^5.3.3",
    "usehooks-ts": "^2.16.0",
    "uuid-random": "^1.3.2",
    "web-vitals": "^2.1.4",
    "zod": "^3.23.8"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "scripts": {
    "start": "craco start",
    "clean": "rm -rf build app dist ui",
    "build": "BUILD_PATH=ui craco build && tsc && electron-builder",
    "build:mac": "sql/scripts/create-db.sh && yarn build -m",
    "build:linux": "sql/scripts/create-db.sh && docker run --rm -it -v $(pwd):/project electronuserland/builder /bin/bash -c \"./buildInDocker.sh $(id -u $(whoami)) -l\"",
    "build:win": "sql/scripts/create-db.sh && docker run --rm -it -v $(pwd):/project electronuserland/builder:wine /bin/bash -c \"./buildInDocker.sh $(id -u $(whoami)) -w\"",
    "build:mac:fast": "sql/scripts/create-db.sh --no-remove && yarn build -m",
    "build:linux:fast": "sql/scripts/create-db.sh --no-remove && docker run --rm -it -v $(pwd):/project electronuserland/builder /bin/bash -c \"./buildInDocker.sh $(id -u $(whoami)) -l\"",
    "build:win:fast": "sql/scripts/create-db.sh --no-remove && docker run --rm -it -v $(pwd):/project electronuserland/builder:wine /bin/bash -c \"./buildInDocker.sh $(id -u $(whoami)) -w\"",
    "test-ui": "craco test",
    "test-electron": "tsc && export NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" && jest --config jest.config.ts",
    "eject": "craco eject",
    "prettier": "prettier --write .",
    "prepublishOnly": "rm -rf dist/ && rollup -c",
    "dev-electron": "sql/scripts/create-db.sh --no-remove && tsc && concurrently -k \"tsc -W\" \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "storybook": "STORYBOOK_DISABLE_TELEMETRY=true storybook dev -p 6006",
    "build-storybook": "STORYBOOK_DISABLE_TELEMETRY=true storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@kayahr/jest-electron-runner": "^29.13.0",
    "@rollup/plugin-typescript": "^8.3.1",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.5.3",
    "@storybook/preset-create-react-app": "^7.5.3",
    "@storybook/react": "^7.5.3",
    "@storybook/react-webpack5": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^13.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/electron": "^1.6.10",
    "@types/jest": "^27.4.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.77",
    "@types/react-dom": "^18",
    "concurrently": "^8.2.1",
    "electron": "^27.0.0",
    "electron-build": "^0.0.3",
    "electron-builder": "^24.9.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "eslint-plugin-storybook": "^0.6.15",
    "prettier": "^2.6.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^2.70.1",
    "rollup-plugin-postcss": "^4.0.2",
    "storybook": "^7.5.3",
    "wait-on": "^7.0.1"
  },
  "resolutions": {
    "@types/react": "^18",
    "@types/react-dom": "^18"
  },
  "files": [
    "ui",
    "app"
  ],
  "main": "app/electron/main.js",
  "build": {
    "appId": "com.biblica.clearaligner",
    "productName": "ClearAligner",
    "extends": null,
    "extraMetadata": {
      "main": "app/electron/main.js"
    },
    "directories": {
      "buildResources": "build/",
      "app": "."
    },
    "extraFiles": [
      {
        "from": "sql/projects/clear-aligner-default.sqlite",
        "to": "sql/projects/clear-aligner-default.sqlite"
      },
      {
        "from": "sql/clear-aligner-template.sqlite",
        "to": "sql/clear-aligner-template.sqlite"
      },
      {
        "from": "sql/clear-aligner-user.sqlite",
        "to": "sql/clear-aligner-user.sqlite"
      },
      {
        "from": "app/electron/typeorm-migrations",
        "to": "sql/typeorm-migrations"
      }
    ]
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      [
        "@babel/preset-env"
      ],
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "plugins": []
  }
}
